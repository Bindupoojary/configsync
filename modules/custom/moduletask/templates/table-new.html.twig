<table>
   <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>email</th>
      <th>phone number</th>
      <th>gender</th>
    </tr>
   </thead>
{% for row in rows %}
  <tr>
    <td>{{ row.first_name }}</td>
    <td>{{ row.last_name }}</td>
    <td>{{ row.email}}</td>
    <td>{{ row.phone_number }}</td>
    <td>{{ row.gender }}</td>
 </tr>
 {% endfor %}
</table>
